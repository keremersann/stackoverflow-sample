@model List<StackOverFlowClone.ViewModels.CategoryViewModel>

<div class="row">
    <div class="col-sm-12">
        <h4>Categories</h4>
        <ul class="list-group list-group-flush">
            @foreach(var item in Model)
            {
                <li class="list-group-item list-group-item-action" >@item.CategoryID.@item.CategoryName</li>
            }
        </ul>
    </div>
</div>
<script>
    $(document).ready(function () {
        $("a[href='/Home/Categories']").attr("id", "active")
    })
</script>